{
  "name": "ai-swarm-v2",
  "private": true,
  "version": "3.0.1",
  "description": "Production-grade multi-agent AI orchestration system with Temporal.io",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "worker": "pnpm --filter @ai-swarm/worker dev",
    "portal": "pnpm --filter @ai-swarm/portal dev",
    "cli": "pnpm --filter @ai-swarm/cli dev"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}