{
    "name": "@ai-swarm/shared",
    "version": "3.0.1",
    "description": "Shared types, utilities, and LLM client for AI Swarm",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./prompts/*": "./prompts/*"
    },
    "scripts": {
        "build": "tsc && cp -r src/db/migrations dist/db/",
        "dev": "tsc --watch",
        "test": "vitest run",
        "lint": "eslint src/"
    },
    "dependencies": {
        "@simplewebauthn/server": "^11.0.0",
        "@simplewebauthn/types": "^11.0.0",
        "ioredis": "^5.3.0",
        "js-yaml": "^4.1.0",
        "pg": "^8.11.3",
        "pino": "^8.17.0"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20.10.0",
        "@types/pg": "^8.10.9",
        "pino-pretty": "^10.3.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    }
}